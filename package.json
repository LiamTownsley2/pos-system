{
  "name": "pos-system",
  "version": "1.0.14",
  "description": "An Electron application with React and TypeScript for a POS System.",
  "main": "./out/main/index.js",
  "author": "liamtownsley.me",
  "homepage": "https://liamtownsley.me/",
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "LiamTownsley2",
        "repo": "pos-system"
      }
    ]
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && flowbite-react build && electron-vite build",
    "postinstall": "electron-builder install-app-deps && flowbite-react patch && flowbite-react register",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win --publish always",
    "build:mac": "electron-vite build && electron-builder --mac --publish always",
    "build:linux": "electron-vite build && electron-builder --linux --publish always",
    "flowbite:build": "flowbite-react build",
    "flowbite:dev": "flowbite-react dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run flowbite:dev\""
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@heroicons/react": "^2.2.0",
    "electron-updater": "^6.6.2",
    "flowbite": "^3.1.2",
    "flowbite-react": "^0.11.7"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@tailwindcss/postcss": "^4.1.8",
    "@tailwindcss/vite": "^4.1.8",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "electron": "^35.1.5",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.1.0",
    "eslint": "^9.24.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.3",
    "vite": "^6.2.6"
  }
}
